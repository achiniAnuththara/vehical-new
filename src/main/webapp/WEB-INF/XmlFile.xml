<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


     <servlet>
     	<servlet-name> InsertFuelServlet</servlet-name>
     	<servlet-class>OnlineVehicleServicePackage.InsertFuelServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>InsertFuelServlet</servlet-name>
     	<url-pattern>/InsertFuelServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name> GetAllServlet</servlet-name>
     	<servlet-class>OnlineVehicleServicePackage.GetAllServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>GetAllServlet</servlet-name>
     	<url-pattern>/GetAllServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name> UpdateServlet</servlet-name>
     	<servlet-class>OnlineVehicleServicePackage.UpdateServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>UpdateServlet</servlet-name>
     	<url-pattern>/UpdateServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name> DeleteServlet</servlet-name>
     	<servlet-class>OnlineVehicleServicePackage.DeleteServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>DeleteServlet</servlet-name>
     	<url-pattern>/DeleteServlet</url-pattern>
     </servlet-mapping>
     


</web-app>